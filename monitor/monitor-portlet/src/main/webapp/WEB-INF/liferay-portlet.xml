<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC
        "-//Liferay//DTD Portlet Application 6.2.0//EN"
        "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
    <portlet>
        <portlet-name>1</portlet-name>
        <icon>/icon.png</icon>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
    </portlet>
    <portlet>
        <portlet-name>2</portlet-name>
        <icon>/icon.png</icon>
        <scheduler-entry>
            <scheduler-event-listener-class>com.surwing.job.NodeStatusCheckJob</scheduler-event-listener-class>
            <trigger>
                <cron>
                    <cron-trigger-value>0 * * * * ?</cron-trigger-value>
                </cron>
            </trigger>
        </scheduler-entry>
        <scheduler-entry>
            <scheduler-event-listener-class>com.surwing.job.ESBGetPatientInfoJob</scheduler-event-listener-class>
            <trigger>
                <cron>
                    <cron-trigger-value>10 */15 * * * ?</cron-trigger-value>
                </cron>
            </trigger>
        </scheduler-entry>
        <scheduler-entry>
            <scheduler-event-listener-class>com.surwing.job.ESBReportPatientHeatJob</scheduler-event-listener-class>
            <trigger>
                <cron>
                    <cron-trigger-value>20 0 * * * ?</cron-trigger-value>
                </cron>
            </trigger>
        </scheduler-entry>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
    </portlet>
    <portlet>
        <portlet-name>3</portlet-name>
        <icon>/icon.png</icon>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
    </portlet>
    <portlet>
        <portlet-name>4</portlet-name>
        <icon>/icon.png</icon>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
    </portlet>
    <portlet>
        <portlet-name>5</portlet-name>
        <icon>/icon.png</icon>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
    </portlet>
    <portlet>
        <portlet-name>6</portlet-name>
        <icon>/icon.png</icon>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
    </portlet>
    <portlet>
        <portlet-name>7</portlet-name>
        <icon>/icon.png</icon>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
    </portlet>
    <portlet>
        <portlet-name>8</portlet-name>
        <icon>/icon.png</icon>
        <friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
        <friendly-url-mapping>patients_status</friendly-url-mapping>
        <friendly-url-routes>friendly-url-routes/patients_status.xml</friendly-url-routes>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
        <footer-portlet-javascript>/js/echarts.common.min.js</footer-portlet-javascript>
    </portlet>
    <portlet>
        <portlet-name>9</portlet-name>
        <icon>/icon.png</icon>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
    </portlet>
    <portlet>
        <portlet-name>10</portlet-name>
        <icon>/icon.png</icon>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
        <css-class-wrapper>system-parameter</css-class-wrapper>
    </portlet>
    <portlet>
        <portlet-name>11</portlet-name>
        <icon>/icon.png</icon>
        <friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
        <friendly-url-mapping>user</friendly-url-mapping>
        <friendly-url-routes>friendly-url-routes/user.xml</friendly-url-routes>
        <header-portlet-css>/css/main.css</header-portlet-css>
        <footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
    </portlet>
