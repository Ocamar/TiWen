<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.2.0//EN"
        "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_2_0.dtd">
<routes>
    <route>
        <pattern>/user/location-name/{uid}</pattern>
        <implicit-parameter name="p_p_lifecycle">2</implicit-parameter>
        <implicit-parameter name="struts.portlet.mode">view</implicit-parameter>
        <implicit-parameter name="struts.portlet.action">/user/getUserLocationName</implicit-parameter>
    </route>
</routes>